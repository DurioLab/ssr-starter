{
  "name": "ssr-starter",
  "version": "1.0.0",
  "description": "ssr",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "serve": "cd dist && NODE_ENV=production node server.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.15.2",
    "vue": "^2.3.2",
    "vue-router": "^2.5.3",
    "vue-server-renderer": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.5.2",
    "opn": "^5.0.0",
    "ora": "^1.2.0",
    "postcss-loader": "^2.0.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "vue-loader": "^12.0.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.2",
    "webpack": "^2.5.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.5.4"
  }
}
